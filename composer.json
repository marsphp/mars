{
  "name": "mars/mars",
  "description": "The skeleton application for the Mars framework.",
  "keywords": ["mars", "framework", "marsphp"],
  "license": "MIT",
  "$schema": "https://getcomposer.org/schema.json",
  "require": {
    "php": "^8.2",
    "adbario/php-dot-notation": "^3.3",
    "laminas/laminas-diactoros": "^3.5",
    "laminas/laminas-httphandlerrunner": "^2.11",
    "league/container": "^4.2",
    "league/route": "^6.2",
    "spatie/ignition": "^1.15",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "files": [
      "framework/Mars/Support/helpers.php"
    ],
    "psr-4": {
      "App\\": "src/",
      "Mars\\": "framework/Mars/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
