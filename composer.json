{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "mars/mars",
  "description": "The skeleton application for the Mars framework.",
  "keywords": ["mars", "framework", "marsphp"],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "mars/facades": "@dev",
    "mars/framework": "dev-main",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/marsphp/framework.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/marsphp/facades.git"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
