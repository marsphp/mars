{
  "name": "mars/framework",
  "description": "The Mars Framework.",
  "keywords": ["framework", "mars", "marsphp"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Hassane Dao",
      "email": "hassane.dao@marsphp.com"
    },
    {
      "name": "Houssene Dao",
      "email": "houssene.dao@marsphp.com"
    }
  ],
  "autoload": {
    "files": [
      "src/Mars/Support/helpers.php",
      "src/Mars/Filesystem/functions.php"
    ],
    "psr-4": {
      "Mars\\": "src/Mars"
    }
  },
  "require": {
    "php": "^8.2",
    "adbario/php-dot-notation": "^3.3",
    "laminas/laminas-diactoros": "^3.5",
    "laminas/laminas-httphandlerrunner": "^2.11",
    "spatie/ignition": "^1.15",
    "league/container": "^4.2",
    "vlucas/phpdotenv": "^5.6",
    "league/event": "^3.0",
    "league/route": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "php-http/discovery": false
    }
  },
  "prefer-stable": true
}
